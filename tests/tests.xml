<?xml version="1.0" encoding="UTF-8"?>
<testdefinition version="1.0">
   <suite name="jolla-notes-tests" domain="ui">
       <description>Jolla Notes automatic tests</description>
       <set name="feature-tests" feature="Jolla Notes">
           <description>Collection of Jolla Notes tests</description>
           <case manual="false" name="qmltestrunner">
               <step>sleep 60</step>
               <step>/usr/sbin/mcetool -D on</step>
               <step>cd /opt/tests/jolla-notes/ &amp;&amp; XDG_DATA_HOME=/tmp/testdata DISPLAY=:0 qmltestrunner</step>
               <step>/usr/sbin/mcetool -D off</step>
           </case>
       </set>
   </suite>
</testdefinition>
